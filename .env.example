# Порт для сервера
PORT=3000

# Настройки базы данных
DB_PATH=./database.sqlite

# Настройки JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# URL клиента (для CORS)
CLIENT_URL=https://yourdomain.com

# Режим окружения
NODE_ENV=production

# URL API для фронтенда
REACT_APP_API_URL=https://yourdomain.com/api 